{
    "private": true,
    "scripts": {
        "start": "umi dev",
        "test": "umi test",
        "lint": "eslint {src,mock,tests}/**/*.{ts,tsx} --fix",
        "lint:es": "eslint --ext .js,jsx,ts,tsx src",
        "lint:tsc": "tsc -p tsconfig.json --noEmit",
        "build": "father-build build",
        "fa:build": "father build",
        "precommit": "lint-staged",
        "dev": "father-doc dev",
        "doc:build": "father-doc build"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.6.0",
        "@types/jest": "^24.0.13",
        "@umijs/fabric": "^2.0.0-beta.4",
        "antd": "^4.0.0-rc.3",
        "babel-plugin-import": "^1.13.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.7.2",
        "father": "^2.29.2",
        "father-build": "^1.14.2",
        "lerna": "^3.6.0",
        "lerna-changelog": "^0.8.2",
        "prettier": "^1.19.1",
        "umi-test": "^1.5.12",
        "lint-staged": "^7.2.2",
        "umi-types": "^0.5.3"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.{js,jsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "license": "MIT"
